    "@actions/github": "^2.0.0",
    "aws-sdk": "^2.588.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^6.7.2",
    "husky": "^4.1.0",
